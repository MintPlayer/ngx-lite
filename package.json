{
  "name": "ng6-test",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run lint && npm run prettier && ng build --prod && ng test --single && npm run e2e && npm run package",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e --aot",
    "deploy": "ng build --prod && firebase deploy",
    "coverage": "ng test --watch=false --code-coverage",
    "prettier": "run-s prettier:*",
    "prettier:scss": "prettier --parser=scss --single-quote --write \"src/**/*.scss\"",
    "prettier:typescript": "prettier --parser=typescript --single-quote --write \"src/**/*.ts\"",
    "package": "run-s package:*",
    "package:ngx-carbon-ad": "ng-packagr -p src/lib/ngx-carbon-ad/ng-package.json",
    "package:ngx-code-example": "ng-packagr -p src/lib/ngx-code-example/ng-package.json",
    "package:ngx-debounce-click": "ng-packagr -p src/lib/ngx-debounce-click/ng-package.json",
    "package:ngx-eq": "ng-packagr -p src/lib/ngx-eq/ng-package.json",
    "package:ngx-in-viewport": "ng-packagr -p src/lib/ngx-in-viewport/ng-package.json",
    "package:ngx-input-datepicker": "ng-packagr -p src/lib/ngx-input-datepicker/ng-package.json",
    "package:ngx-input-range": "ng-packagr -p src/lib/ngx-input-range/ng-package.json",
    "package:ngx-input-star-rating": "ng-packagr -p src/lib/ngx-input-star-rating/ng-package.json",
    "package:ngx-input-switch": "ng-packagr -p src/lib/ngx-input-switch/ng-package.json",
    "package:ngx-input-tag": "ng-packagr -p src/lib/ngx-input-tag/ng-package.json",
    "package:ngx-json-ld": "ng-packagr -p src/lib/ngx-json-ld/ng-package.json",
    "package:ngx-loaders": "ng-packagr -p src/lib/ngx-loaders/ng-package.json",
    "package:ngx-modal": "ng-packagr -p src/lib/ngx-modal/ng-package.json",
    "package:ngx-nav-drawer": "ng-packagr -p src/lib/ngx-nav-drawer/ng-package.json",
    "package:ngx-progress": "ng-packagr -p src/lib/ngx-progress/ng-package.json",
    "package:ngx-tabs": "ng-packagr -p src/lib/ngx-tabs/ng-package.json"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/animations": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "blueprint-css": "^3.0.0-beta.4",
    "core-js": "^2.5.4",
    "date-fns": "^1.29.0",
    "normalize.css": "^8.0.0",
    "prismjs": "^1.14.0",
    "resize-observer-polyfill": "^1.5.0",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^6.0.0",
    "@angular-devkit/build-ng-packagr": "~0.6.0",
    "@angular-devkit/build-angular": "~0.6.0",
    "ng-packagr": "^3.0.0-rc.2",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "typescript": "~2.7.2",
    "@angular/cli": "~6.0.0",
    "@angular/language-service": "^6.0.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "prettier": "1.12.1",
    "npm-run-all": "^4.1.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1"
  }
}
